    "length = 50\n",
    "fib_seq = [1, 2]\n",
    "for i in range(2, length):\n",
    "    next_num = (fib_seq[-1] + fib_seq[-2])\n",
    "    if (next_num < 4000000):\n",
    "        fib_seq.append(next_num)\n",
    "    else:\n",
    "        break\n",
    "\n",
    "fib_sum = 0\n",
    "for i in fib_seq:\n",
    "    if i % 2 == 0:\n",
    "        fib_sum += i        \n",
    "print fib_sum"
   ]
  }
