    "import numpy as np\n",
    "\n",
    "summ = 0\n",
    "\n",
    "for i in range(2,2000001):\n",
    "    rangee = np.arange(2,i)\n",
    "    div_range = i % rangee\n",
    "    true_prime = 0\n",
    "    for div in div_range:\n",
    "        if div == 0:\n",
    "            true_prime = 1\n",
    "    if true_prime == 0:\n",
    "        summ += i\n",
    "        \n",
    "print summ"
   ]
  }
